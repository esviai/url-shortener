<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <% include ../partials/header.ejs %>
  </head>
  <body>
    <div class="container">
      <% include ../partials/title.ejs %>
      <div class="row">
      </div>
      <div class="row">
      </div>
    </div>
    <div class="container">
      <div class="row">
        <div class="col s12">
          <table class="striped">
            <thead>
              <tr>
                <th class="center-align">URL</th>
                <th class="center-align">Shortened URL</th>
                <th class="center-align">Click Count</th>
                <!--<th class="center-align">Options</th>-->
              </tr>
            </thead>
            <tbody>
              <% urls.forEach(url => { %>
              <tr style=>
                <td class="center-align"><%= url.Url %></td>
                <td class="center-align"><a class="waves-effect waves-light btn blue-lighten-5" style="text-transform:none" href= <%- '/bl/'+ url.ShortUrl %>><%= 'bl/'+ url.ShortUrl %></a></td>
                <td class="center-align"><%= url.click_count %></td>
                <!--<td class="center-align"><a class="waves-effect waves-light btn" href="./url/edit/<%- url.ShortUrl %>">Edit</a><br></br><a class="waves-effect waves-light btn red" href="./user/delete/<%- url.ShortUrl %>" onclick="return confirm('Are you sure')">Delete</a></td>-->
              </tr>
              <% }) %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
    <div class="row">
    </div>
    <div class="container">
      <div class="row">
        <div ><a href=""></a></div>
      </div>
    </div>
  </body>
</html>
